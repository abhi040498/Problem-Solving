#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int x, y, z,t;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%d%d%d",&x,&y,&z);

        if(abs(x-z)<abs(y-z))
    {
        printf("Cat A\n");
    }
    else if(abs(x-z)>abs(y-z))
    {
        printf("Cat B\n");
    }
    else
    {
        printf("Mouse C\n");
    }

    }

    return 0;
}
