#include <bits/stdc++.h>

using namespace std;

int main(){
    int n;
    float p=0,m=0,z=0;
    cin >> n;
    int arr[n];
    int sum=0;

    for (int i=0;i<n;i++){
        cin >>arr[i];
        if (arr[i]>0) 
        p++;
        else if(arr[i]<0)
        m++;
        else
        z++;
    }
    cout << p/n<<"\n"<<m/n<<"\n"<<z/n;
}
