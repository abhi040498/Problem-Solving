#include <stdio.h>
#include <stdlib.h>


// Complete the findDigits function below.

int main()
{
     int t,n;
    int count=0,rem;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%d",&n);
     int a=n;
    while(a)
    {
            rem=a % 10;
        if((a % 10 != 0) && (n % rem == 0))
        {
            count++;
        }
        a/=10;
    }
    printf("%d\n", count);
    count=0;
    }

    return 0;
}
