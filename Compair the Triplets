#include <bits/stdc++.h>

using namespace std;

int main(){
    int k,a=0,b=0;
    //cin >> n;
     
       int arr1[3]; int arr2[3];

    for (int i=0;i<3;i++){
        cin >>k;
        if (k>0 && k<=100)
        arr1[i]=k;
    }
    for (int i=0;i<3;i++){
        cin >>k;
        if (k>0 && k<=100)
        arr2[i]=k;
  
    }
     
     for (int i=0;i<3;i++){
       if (arr1[i]> arr2[i]) {
           a+=1;
       }
       else if (arr1[i]<arr2[i]) {
           b+=1;
         }
     }
    
    cout << a<<" "<<b;
}
